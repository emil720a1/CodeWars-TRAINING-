//okey explanation
function solution(number){
    var sum = 0;

    for(var i = 1;i< number; i++){
        if(i % 3 == 0 || i % 5 == 0){ //if number divided by 3 or 5 and % === 0
            sum += i   //We summing all num
        }
    }
    return sum; // we return sum
}


console.log(solution(10)); //like example [3+5+6+9 = 23]
