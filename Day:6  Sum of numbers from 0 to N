const SequenceSum = {
  showSequence: function(n) {
    let arr=[]
    let sum=0;
    for(let i=0;i<=n;i++){
        arr.push(i)
        sum+=arr[i]
    }
    
    let a = (String(arr.join('+')))
if(n>0){
    return `${a} = ${sum}`
}else if(n === 0){
    return `${n} = ${sum}`
}
else{
    return ` ${n} < ${sum}`
}

}
}
